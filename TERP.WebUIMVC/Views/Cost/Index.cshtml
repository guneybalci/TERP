                                            <option>Borç</option>
                                            <option>Taksit</option>
                                            <option>Yakıt</option>
                                            <option>Gider</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Tarih:</label>
                                        <input name="datepicker" class="datepicker-default form-control" placeholder="1 Ocak 1978"
                                               id="datepicker" />
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Para Birimi:</label>
                                        <form action="#">
                                            <div class="input-group">
                                                <input type="text" class="form-control" placeholder="0.00" />
                                                <div class="input-group-append">
                                                    <select class="form-control default-select" id="sel1">
                                                        <option>₺ : TRY</option>
                                                        <option>€ : EUR</option>
                                                        <option>$ : USD</option>
                                                    </select>
